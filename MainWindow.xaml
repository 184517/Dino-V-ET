<Window x:Class="ETstrikesBack.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ETstrikesBack"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
  <Canvas Background="Black">
        <Button x:Name="btnPlay" Content="Play Game" Click="BtnPlay_Click" FontFamily="Impact" FontSize="24" Background="Gold"
                Height="139" Width="481" Canvas.Left="10" Canvas.Top="270"/>
        <Button x:Name="btnScores" Content="See High Scores" Click="BtnScores_Click" FontFamily="Impact" FontSize="24" Background="Aqua"
                Canvas.Top="271" Height="136" Width="286" Canvas.Left="496"/>
        <Button x:Name="btnQuit" Content="Quit" Click="BtnQuit_Click" FontFamily="Impact" FontSize="24" Background="Red"
                Canvas.Left="708" Height="63" Width="84"/>
        <Label x:Name="lblTitle" Content="ET Strikes Back" FontFamily="Impact" FontSize="50" Background="White" 
               Canvas.Left="232" Canvas.Top="10"/>
    </Canvas>
</Window>
