<Window x:Class="ETstrikesBack.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ETstrikesBack"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Canvas>
        <Button x:Name="btnPlay" Content="Play Game" Click="BtnPlay_Click"/>
        <Button x:Name="btnScores" Content="See High Scores" Click="BtnScores_Click" Canvas.Top="44"/>
        <Button x:Name="btnQuit" Content="quit" Click="BtnQuit_Click" Canvas.Left="10" Canvas.Top="102"/>
    </Canvas>
</Window>
